<h1>projects and collaborations</h1>
<br />

<div class="row">
  <% for(var i in projects) { %> <% var project = projects[i] %>
  <a
    href="<%- project.link %>"
    target="_blank"
    class="col-xl-3 col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center project"
  >
    <div
      class="project-image"
      style="background-image: url('/images/projects/<%- project.picture %>')"
    ></div>
    <br />
    <%- project.name %>
  </a>
  <% } %>
</div>

<br />
