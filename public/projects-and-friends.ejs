<h1>CUSTOMERS AND FRIENDS AND PARTNERS</h1>

<div class="row">
  <% for(var i in partners) { %> <% var partner = partners[i] %>
  <a
    href="<%- partner.link %>"
    target="_blank"
    class="col-xl-2 col-lg-2 col-md-3 col-sm-3 col-6 text-center"
  >
    <div
      class="partners-image"
      style="background-image: url('/images/partners/<%- partner.picture %>')"
    ></div>
  </a>
  <% } %>
</div>
